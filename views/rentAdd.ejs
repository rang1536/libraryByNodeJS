                <br/><br/>

				<div class="container">
				  <form class="form-horizontal" role="form" action="/rent/books" method='post'>
				    <fieldset>
			        <div class="form-group">
				        <label class="col-sm-3 control-label" for="booksNumber">도서번호</label>
				        <div class="col-sm-7">
				          <input type="text" class="form-control" name="booksNumber" id="booksNumber" placeholder="도서번호을 입력하세요">
				        </div>
				      </div>
				      <div class="form-group">
				        <label class="col-sm-3 control-label" for="membernumber">회원번호</label>
				        <div class="col-sm-7">
				          <input type="text" class="form-control" name="membernumber" id="membernumber" placeholder="회원번호를 입력하세요">
				        </div>
				      </div>
              <div class="form-group">
  							<label class="col-sm-3 control-label" for="startDay">대여일</label>
  							<div class="col-sm-7">
  							  <input type="date" class="form-control" name="startDay" id="startDay"/>
  							</div>
  						</div>
  						<div class="form-group">
  							<label class="col-sm-3 control-label" for="endDay">반납일</label>
  							<div class="col-sm-7">
  							  <input type="date" class="form-control" name="endDay" id="endDay"/>
  							</div>
  						</div>
  						<div class="form-group">
  							<label class="col-sm-3 control-label" for="rentPay">대여료</label>
  							<div class="col-sm-7">
  							  <input type="text" class="form-control" name="rentPay" id="rentPay" placeholder="선납금을 입력하세요"/>
  							</div>
  						</div>
  						<div class="form-group">
  							<label class="col-sm-3 control-label" for="booksDamage">파손여부</label>
  							<div class="col-sm-7">
  							  <label class="radio-inline">
  							  	<input type="radio"  name="booksDamage" value="1"/>파손됨
  							  </label>
  							  <label class="radio-inline">
  							  	<input type="radio"  name="booksDamage" value="0"/>멀쩡함
  							  </label>
  							</div>
  						</div>
  						<div class="form-group">
				        <div class="col-sm-offset-3 col-sm-9">
				          <input type="submit" class="btn btn-success" value="등록"/>
				          <input type="reset" class="btn btn-success" value="다시입력"/>
				        </div>
				      </div>
				    </fieldset>
				  </form>
